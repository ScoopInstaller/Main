            "hash": "182e9ccbd64107752bfe83ded6ab4fa1b3cff54afd0f76f6435540c68cf6a125"
        },
        "64bit": {
            "url": "https://github.com/vim/vim-win32-installer/releases/download/v8.0.1608/gvim_8.0.1608_x64.zip",
