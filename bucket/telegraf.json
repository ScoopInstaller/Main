    "description": "The plugin-driven server agent for collecting & reporting metrics.",
    "homepage": "https://www.influxdata.com/time-series-platform/telegraf/",
    "license": "MIT",
    "architecture": {
        "64bit": {
            "url": "https://dl.influxdata.com/telegraf/releases/telegraf-1.37.2_windows_amd64.zip",
            "hash": "31b57b2a80f58ebb212d87d6dfa8c31381a6ecd13ce1302996723c1555e7f808"
        },
        "32bit": {
            "url": "https://dl.influxdata.com/telegraf/releases/telegraf-1.37.2_windows_i386.zip",
