    "license": "MIT",
    "architecture": {
        "64bit": {
            "url": "https://dl.influxdata.com/telegraf/releases/telegraf-1.29.0_windows_amd64.zip",
            "hash": "100ec616056e107c4cccdb9b5a0a6f2d0d6e8e896114f623a45af80866538460"
        },
        "32bit": {
            "url": "https://dl.influxdata.com/telegraf/releases/telegraf-1.29.0_windows_i386.zip",
