    "url": "https://github.com/RushiChaganti/psutils/raw/main/cat.ps1",
    "hash": "21F814CA14D7F08ACB6F6601D05389B9296A29F6B78766496FACBC84012873C6",
    "bin": "cat.ps1",
    "checkver": {
        "url": "https://github.com/RushiChaganti/psutils/commits/main/cat.ps1.atom",
        "regex": "(?s)>(\\d+)-(\\d+)-(\\d+)T.*?/(?<sha>[0-9a-f]{40})",
        "replace": "0.${1}.${2}.${3}"
    },
    "autoupdate": {
        "url": "https://github.com/RushiChaganti/psutils/raw/$matchSha/cat.ps1",
        "hash": {
            "url": "https://github.com/RushiChaganti/psutils/raw/$matchSha/cat.ps1.sha256",
            "regex": "$sha256"
        }
    }
}
