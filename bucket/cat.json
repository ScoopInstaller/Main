{
    "version": "1.0.0",
    "description": "A custom cat command implemented in PowerShell",
    "homepage": "https://github.com/RushiChaganti/psutils",
    "license": "MIT",
    "url": "https://raw.githubusercontent.com/RushiChaganti/psutils/main/cat.ps1",
    "hash": {
        "url": "https://github.com/RushiChaganti/psutils/releases/download/v$version/cat.ps1.sha256",
        "regex": "(?<sha>[0-9a-f]{64})"
    },
    "bin": "cat.ps1",
    "checkver": {
        "url": "https://api.github.com/repos/RushiChaganti/psutils/git/refs/tags",
        "regex": "refs/tags/v(\\d+\\.\\d+\\.\\d+)",
        "replace": "$1"
    },
    "autoupdate": {
        "url": "https://github.com/RushiChaganti/psutils/releases/download/v$version/cat.ps1"
    }
}
