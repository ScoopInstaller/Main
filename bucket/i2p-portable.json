{
    "version": "0.9.41",
    "description": "An anonymizing network.",
    "homepage": "https://geti2p.net",
    "license": {
        "identifier": "Freeware",
        "url": "https://github.com/i2p/i2p.i2p/blob/master/LICENSE.txt"
    },
    "url": "https://launchpad.net/i2p/trunk/0.9.41/+download/i2pinstall_0.9.41_windows.exe#/dl.7z",
    "hash": "e8d896e94696c3cb3be20cb28ea902595f82f7028622931a8ab6f451e4a3d6b5",
    "suggest": {
        "JDK": [
            "java/oraclejdk",
            "java/openjdk"
        ]
    },
    "bin": [
        "I2P.exe",
        "I2Psvc.exe"
    ],
    "shortcuts": [
        [
            "I2Psvc.exe",
            "Start I2P (restartable)",
            "-c wrapper.config",
            "docs\\start.ico"
        ]
    ],
    "persist": "i2p",
    "installer": {
        "script": [
            "New-Item -Force -Path \"$env:TEMP\" -Name \"I2P\" -ItemType \"directory\" | Out-Null",
            "$temp = \"$env:TEMP\\I2P\"",
            "Move-Item -Force -Path \"$dir\\install.jar\" -Destination \"$temp\\install.jar\"",
            "New-Item -Force -Path \"$temp\" -Name \"install.defaults\" -Value \"INSTALL_PATH=$($dir.toString().Replace('\\', '/'))\" | Out-Null",
            "java -jar \"$temp\\install.jar\" -auto | Out-Null",
            "Remove-Item -Force -Recurse -Path \"$temp\"",
            "@\"",
            "wrapper.java.additional.3=-Di2p.dir.pid=\"./i2p\"",
            "wrapper.java.additional.3.stripquotes=TRUE",
            "wrapper.java.additional.4=-Di2p.dir.temp=\"./i2p\"",
            "wrapper.java.additional.4.stripquotes=TRUE",
            "wrapper.java.additional.5=-Di2p.dir.config=\"./i2p\"",
            "wrapper.java.additional.5.stripquotes=TRUE",
            "wrapper.logfile=./i2p/wrapper.log",
            "wrapper.java.pidfile=./i2p/routerjvm.pid",
            "wrapper.pidfile=./i2p/i2p.pid",
            "\"@ | Out-File -FilePath \"$dir\\wrapper.config\"  -Encoding UTF8 -Append"
        ]
    },
    "checkver": {
        "url": "https://launchpad.net/i2p/trunk",
        "regex": "Latest version is ([\\d.]+)"
    },
    "autoupdate": {
        "url": "https://launchpad.net/i2p/trunk/$version/+download/i2pinstall_$version_windows.exe#/dl.7z",
        "hash": {
            "url": "https://geti2p.net/download",
            "regex": "$basename(?:.|\\n)*$sha256(?:.|\\n)*Download that file and run it."
        }
    }
}
