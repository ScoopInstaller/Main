{
    "version": "1.1.1k_2",
    "description": "A robust, commercial-grade, and full-featured toolkit for the Transport Layer Security (TLS) and Secure Sockets Layer (SSL) protocols.",
    "homepage": "https://curl.haxx.se/windows/",
    "license": "OpenSSL",
    "architecture": {
        "64bit": {
            "url": "https://curl.haxx.se/windows/dl-7.77.0_2/openssl-1.1.1k_2-win64-mingw.tar.xz",
            "hash": "20b4d9956d047e07cbeaff9c9cee6ccf4f9f3dbd07e7ef0383cb74959d0f2913",
            "extract_dir": "openssl-1.1.1k_2-win64-mingw"
        },
        "32bit": {
            "url": "https://curl.haxx.se/windows/dl-7.77.0_2/openssl-1.1.1k_2-win32-mingw.tar.xz",
            "hash": "84ff6e9df906839a8875380962bc0247397fdc13e32db48a34585c2f7c23e07d",
            "extract_dir": "openssl-1.1.1k_2-win32-mingw"
        }
    },
    "bin": "openssl.exe",
    "env_add_path": ".",
    "env_set": {
        "OPENSSL_CONF": "$dir\\openssl.cnf"
    },
    "checkver": "dl-(?<curl>[\\d._]+)/openssl-([\\da-z._]+)-win64-mingw",
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://curl.haxx.se/windows/dl-$matchCurl/openssl-$version-win64-mingw.tar.xz",
                "extract_dir": "openssl-$version-win64-mingw"
            },
            "32bit": {
                "url": "https://curl.haxx.se/windows/dl-$matchCurl/openssl-$version-win32-mingw.tar.xz",
                "extract_dir": "openssl-$version-win32-mingw"
            }
        },
        "hash": {
            "url": "$baseurl/hashes.txt",
            "regex": "SHA256\\($basename\\)=\\s+$sha256"
        }
    }
}
