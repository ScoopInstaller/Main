        }
    },
    "pre_install": "Get-Item \"$dir\\nexus-*\" | Rename-Item -NewName 'nexus'",
    "bin": "nexus\\bin\\nexus.exe",
    "persist": "sonatype-work",
    "checkver": {
        "url": "https://help.sonatype.com/repomanager3/download",
        "regex": "Download <strong>Nexus Repository ([\\d.]+)</strong>"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://download.sonatype.com/nexus/3/latest-win64.zip"
            }
        },
        "hash": {
            "url": "$url.sha1"
        }
    }
}
