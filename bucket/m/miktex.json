    "installer": {
        "args": [
            "--portable=$dir",
            "--unattended",
            "--private"
        ]
    },
    "persist": "texmfs\\config",
    "checkver": {
        "url": "https://miktex.org/download",
        "regex": "basic-miktex-([\\d.]+)-x64\\.exe"
    },
    "autoupdate": {
        "architecture": {
