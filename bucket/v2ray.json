            "hash": "sha512:e7e6516fd19c4c0e1a6b0810139a418912739f3a0bfac4c11ca22584cd0b3f9c02ed9a7937de3e74d51efabb0ce68ca954ab94f66dd787a2811685a3c82862b8"
        },
        "32bit": {
