    "post_install": "if (Test-Path \"$(appdir curl $global)\") { Copy-Item \"$dir\\cacert.pem\" \"$(appdir curl $global)\\current\\bin\\curl-ca-bundle.crt\" }",
    "checkver": "cacert-([\\d-]+)\\.pem",
    "autoupdate": {
        "url": "https://curl.haxx.se/ca/cacert-$version.pem#/cacert.pem"
    }
