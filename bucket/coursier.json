{
    "version": "2.1.2",
    "description": "Pure Scala Artifact Fetching",
    "homepage": "https://github.com/coursier/coursier",
    "license": "Apache-2.0",
    "url": "https://github.com/coursier/coursier/releases/download/v2.1.2/coursier#/coursier.jar",
    "hash": "5ae667c151727074f353fdf1d3b61de5d722af074733ba80551ec9ab466a9ffe",
    "bin": "coursier.jar",
    "env_set": {
        "COURSIER_CACHE": "$dir\\cache",
        "COURSIER_BIN_DIR": "$dir\\bin"
    },
    "env_add_path": "bin",
    "persist": [
        "cache",
        "bin"
    ],
    "checkver": {
        "url": "https://github.com/coursier/coursier/releases",
