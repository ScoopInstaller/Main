        },
        "arm64": {
            "url": "https://github.com/microsoft/garnet/releases/download/v1.0.18/win-arm64-based-readytorun.zip",
            "hash": "152e60938769486d83553b39f21be440f4cc060c2d59a2bd3a7821829adcaef5"
        }
    },
    "bin": "GarnetServer.exe",
    "persist": [
        "garnet.conf",
        "config"
    ],
    "checkver": {
        "github": "https://github.com/microsoft/garnet"
    },
    "autoupdate": {
        "architecture": {
