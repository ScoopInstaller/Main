        },
        "arm64": {
            "url": "https://github.com/microsoft/garnet/releases/download/v1.0.12/win-arm64-based-readytorun.zip",
            "hash": "109cec21c533263b92f8a235c9e8973cbbc4ebc14b94fd77531d13dcd9daa1c2"
        }
    },
    "bin": "GarnetServer.exe",
    "persist": [
        "garnet.conf",
        "config"
    ],
    "checkver": {
        "github": "https://github.com/microsoft/garnet"
    },
    "autoupdate": {
        "architecture": {
