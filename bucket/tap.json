        "script": "Invoke-ExternalCommand -FilePath \"$dir\\tapinstall.exe\" -ArgumentList \"remove TAP0901\" -RunAs | Out-Null"
    },
    "checkver": {
        "url": "https://raw.githubusercontent.com/OpenVPN/tap-windows6/master/version.m4",
        "regex": "define\\(\\[PRODUCT_VERSION\\], \\[([\\d.]+)\\]\\)"
    },
    "autoupdate": {
        "url": "https://build.openvpn.net/downloads/releases/tap-windows-$version.zip",
        "architecture": {
            "64bit": {
                "extract_dir": "tap-windows-$version\\amd64"
            },
            "32bit": {
                "extract_dir": "tap-windows-$version\\i386"
            }
        }
    }
