{
    "version": "1.25.5",
    "description": "An open source programming language that makes it easy to build simple, reliable, and efficient software.",
    "homepage": "https://golang.org",
    "license": "BSD-3-Clause",
    "architecture": {
        "64bit": {
            "url": "https://dl.google.com/go/go1.25.5.windows-amd64.zip",
            "hash": "ae756cce1cb80c819b4fe01b0353807178f532211b47f72d7fa77949de054ebb"
        },
        "32bit": {
            "url": "https://dl.google.com/go/go1.25.5.windows-386.zip",
            "hash": "a593393ea7715ffd315158f622a76226c3a4c4a0a6f92b1aeae03d7380cc06a3"
        },
        "arm64": {
            "url": "https://dl.google.com/go/go1.25.5.windows-arm64.zip",
            "hash": "55a94a423a6b8f3ac2ac4d05a6e44d7760c6520a2c6dcef7425f6bac79c4eece"
        }
    },
    "extract_dir": "go",
    "installer": {
        "script": [
            "$envgopath = \"$env:USERPROFILE\\go\"",
            "if ($env:GOPATH) { $envgopath = $env:GOPATH }",
            "info \"Adding '$envgopath\\bin' to PATH...\"",
            "Add-Path -Path \"$envgopath\\bin\" -Global:$global -Force"
        ]
    },
    "uninstaller": {
        "script": [
            "$envgopath = \"$env:USERPROFILE\\go\"",
            "if ($env:GOPATH) { $envgopath = $env:GOPATH }",
            "info \"Removing '$envgopath\\bin' from PATH...\"",
            "Remove-Path -Path \"$envgopath\\bin\" -Global:$global"
        ]
    },
    "bin": [
        "bin\\go.exe",
        "bin\\gofmt.exe"
    ],
    "checkver": {
        "url": "https://golang.org/dl/",
        "regex": "go([\\d.]+)\\.windows-"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://dl.google.com/go/go$version.windows-amd64.zip"
            },
            "32bit": {
                "url": "https://dl.google.com/go/go$version.windows-386.zip"
            },
            "arm64": {
                "url": "https://dl.google.com/go/go$version.windows-arm64.zip"
            }
        },
        "hash": {
            "url": "$url.sha256"
        }
    }
}
