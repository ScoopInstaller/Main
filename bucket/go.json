{
    "version": "1.25.3",
    "description": "An open source programming language that makes it easy to build simple, reliable, and efficient software.",
    "homepage": "https://golang.org",
    "license": "BSD-3-Clause",
    "architecture": {
        "64bit": {
            "url": "https://dl.google.com/go/go1.25.3.windows-amd64.zip",
            "hash": "bc249a599c6fe9d0d4093c363856f6c6320dbbe05e5d5d8818b711fb4a14fc23"
        },
        "32bit": {
            "url": "https://dl.google.com/go/go1.25.3.windows-386.zip",
            "hash": "4d2b1478b2c9bdab69541cef43ad3bde53a755ae06764fdb12d8a6b724908170"
        },
        "arm64": {
            "url": "https://dl.google.com/go/go1.25.3.windows-arm64.zip",
            "hash": "54aeaffcb70b4269fc97a325319640f44a0dc16b58cce39463014d5be1014233"
        }
    },
    "extract_dir": "go",
    "installer": {
        "script": [
            "$envgopath = \"$env:USERPROFILE\\go\"",
            "if ($env:GOPATH) { $envgopath = $env:GOPATH }",
            "info \"Adding '$envgopath\\bin' to PATH...\"",
            "Add-Path -Path \"$envgopath\\bin\" -Global:$global -Force"
        ]
    },
    "uninstaller": {
        "script": [
            "$envgopath = \"$env:USERPROFILE\\go\"",
            "if ($env:GOPATH) { $envgopath = $env:GOPATH }",
            "info \"Removing '$envgopath\\bin' from PATH...\"",
            "Remove-Path -Path \"$envgopath\\bin\" -Global:$global"
        ]
    },
    "bin": [
        "bin\\go.exe",
        "bin\\gofmt.exe"
    ],
    "checkver": {
        "url": "https://golang.org/dl/",
        "regex": "go([\\d.]+)\\.windows-"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://dl.google.com/go/go$version.windows-amd64.zip"
            },
            "32bit": {
                "url": "https://dl.google.com/go/go$version.windows-386.zip"
            },
            "arm64": {
                "url": "https://dl.google.com/go/go$version.windows-arm64.zip"
            }
        },
        "hash": {
            "url": "$url.sha256"
        }
    }
}
