{
    "version": "4.2.2",
    "description": "Static analysis tool for Java",
    "homepage": "https://spotbugs.github.io/",
    "license": "LGPL-2.1-only",
    "suggest": {
        "JDK": [
            "java/oraclejdk",
            "java/openjdk"
        ]
    },
    "url": "https://repo.maven.apache.org/maven2/com/github/spotbugs/spotbugs/4.2.2/spotbugs-4.2.2.zip",
    "hash": "fbbfca013d11dfecd8b5fb6aa9f3d3f1b6a2689e5a1cd93afe4ffd99de7a8505",
    "extract_dir": "spotbugs-4.2.2",
    "bin": "bin\\spotbugs.bat",
    "shortcuts": [
        [
            "bin\\spotbugs.bat",
            "SpotBugs",
            "",
            "bin\\spotbugs.ico"
        ]
    ],
    "persist": "plugin",
    "checkver": {
        "url": "https://spotbugs.readthedocs.io/en/stable/installing.html",
        "regex": "spotbugs ([\\d.]+) documentation"
    },
    "autoupdate": {
        "url": "https://repo.maven.apache.org/maven2/com/github/spotbugs/spotbugs/$version/spotbugs-$version.zip",
        "extract_dir": "spotbugs-$version"
    }
}
