{
    "homepage": "https://nmap.org",
    "description": "Network exploration and security auditing utility. (includes Npcap)",
    "version": "7.80",
    "license": {
        "identifier": "GPL-2.0-only",
        "url": "https://svn.nmap.org/nmap/COPYING"
    },
    "url": "https://nmap.org/dist/nmap-7.80-setup.exe#/dl.7z",
    "hash": "3b4d726bd366e7439367fa78a186dfa9b641d3b2ad354fd915581b6567480f94",
    "pre_install": "Move-Item \"$dir\\`$PLUGINSDIR\\npcap*.exe\" \"$dir\\`$PLUGINSDIR\\npcap.exe\"",
    "installer": {
        "script": "Invoke-ExternalCommand -FilePath \"$dir\\`$PLUGINSDIR\\npcap.exe\" -ArgumentList \"/S /D=$dir\\npcap\" -RunAs | Out-Null"
    },
    "uninstaller": {
        "script": "Invoke-ExternalCommand -FilePath \"$dir\\npcap\\Uninstall.exe\" -ArgumentList '/S' -RunAs | Out-Null"
    },
    "bin": [
        "nmap.exe",
        "ncat.exe",
        "ndiff.exe",
        "zenmap.exe",
        "nping.exe"
    ],
    "shortcuts": [
        [
            "zenmap.exe",
            "Nmap - Zenmap GUI"
        ]
    ],
    "post_install": "Remove-Item \"$dir\\`$PLUGINSDIR\", \"$dir\\Uninstall.exe\" -Recurse -Force",
    "notes": "'Npcap' has been installed automatically.",
    "checkver": {
        "url": "https://nmap.org/download.html",
        "regex": "nmap-([\\d.]+)-setup"
    },
    "autoupdate": {
        "url": "https://nmap.org/dist/nmap-$version-setup.exe#/dl.7z"
    },
    "suggest": {
        "vcredist": "extras/vcredist2013"
    }
}
