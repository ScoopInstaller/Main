    },
    "checkver": {
        "url": "https://chocolatey.org/packages/nunit-console-runner",
        "regex": "\\s+([\\d.]+)</title>"
    },
    "autoupdate": {
        "url": "https://packages.chocolatey.org/nunit-console-runner.$version.nupkg",
        "hash": {
            "url": "https://chocolatey.org/packages/nunit-console-runner",
            "regex": "$sha256.*?$basename"
