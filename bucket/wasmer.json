{
    "version": "1.0.0-alpha3",
    "description": "Universal WebAssembly Runtime supporting WASI and Emscripten",
    "homepage": "https://wasmer.io/",
    "license": "MIT",
    "architecture": {
        "64bit": {
            "url": "https://github.com/wasmerio/wasmer/releases/download/1.0.0-alpha3/wasmer-windows.exe",
            "hash": "9975c76ec8036e50843f397950e4840084053741b3dcb1dea7acb3a509ce6a39"
        }
    },
    "env_set": {
        "WASMER_DIR": "$dir",
        "WASMER_CACHE_DIR": "$dir\\cache"
    },
    "bin": [
        "bin\\wax.cmd",
        "bin\\wapm.exe",
        "bin\\wasmer.exe"
    ],
    "innosetup": true,
    "checkver": {
        "github": "https://github.com/wasmerio/wasmer",
        "regex": "tag/([\\w.-]+)"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/wasmerio/wasmer/releases/download/$version/wasmer-windows.exe"
            }
        }
    }
}
