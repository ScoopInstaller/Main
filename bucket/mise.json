            "hash": "102830b09e25b0c1f8f8a402a7e6ef1f20364fd90fc3533524305c1b91fd58f1"
        },
        "arm64": {
            "url": "https://github.com/jdx/mise/releases/download/v2025.2.7/mise-v2025.2.7-windows-arm64.zip",
