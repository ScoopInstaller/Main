    "checkver": {
        "url": "https://www.jruby.org/download",
        "re": "Current Release:\\s+JRuby\\s+([\\d.]+)"
    },
    "autoupdate": {
        "url": "https://s3.amazonaws.com/jruby.org/downloads/$version/jruby-bin-$version.zip",
