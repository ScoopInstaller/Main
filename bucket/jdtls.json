    "license": "EPL-2.0",
    "suggest": {
        "JDK": "java/openjdk",
        "Python": "python"
    },
    "url": "https://download.eclipse.org/jdtls/snapshots/jdt-language-server-1.17.0-202210120228.tar.gz",
    "hash": "69443aa51e01296d14f99fe2d1f2827c9777b740c07e1194988a7a1c1114b062",
    "installer": {
        "script": "shim (search_in_path python.exe) $global jdtls \"$dir\\bin\\jdtls\""
    },
    "uninstaller": {
        "script": "rm_shim jdtls (shimdir $global) jdtls"
    },
    "checkver": {
        "url": "https://download.eclipse.org/jdtls/snapshots/latest.txt",
        "regex": "jdt-language-server-([\\d.]+)-(\\d+)",
        "replace": "${1}-${2}"
    },
    "autoupdate": {
        "url": "https://download.eclipse.org/jdtls/snapshots/jdt-language-server-$match1-$match2.tar.gz",
        "hash": {
            "url": "$url.sha256"
        }
    }
}
