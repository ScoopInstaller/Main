    "license": "EPL-2.0",
    "suggest": {
        "JDK": "java/openjdk",
        "Python": "python"
    },
    "url": "https://download.eclipse.org/jdtls/snapshots/jdt-language-server-1.31.0-202312111429.tar.gz",
    "hash": "9c86a1164b088c497c076edf2929fefab41903440582efcac478f5b135af90c5",
    "installer": {
        "script": "shim (search_in_path python.exe) $global jdtls \"$dir\\bin\\jdtls\""
    },
    "uninstaller": {
        "script": "rm_shim jdtls (shimdir $global) jdtls"
    },
    "checkver": {
        "url": "https://download.eclipse.org/jdtls/snapshots/latest.txt",
        "regex": "jdt-language-server-([\\d.]+)-(\\d+)",
        "replace": "${1}-${2}"
    },
    "autoupdate": {
        "url": "https://download.eclipse.org/jdtls/snapshots/jdt-language-server-$match1-$match2.tar.gz",
        "hash": {
            "url": "$url.sha256"
        }
    }
}
