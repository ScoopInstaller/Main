        }
    },
    "extract_dir": "PFiles\\psqlODBC\\1700",
    "env_add_path": "bin",
    "checkver": {
        "url": "https://www.postgresql.org/ftp/odbc/releases/",
        "regex": "REL-(?<major>\\d+)_(?<minor>\\d+)_(?<patch>\\d+)",
        "replace": "${major}.${minor}.${patch}"
