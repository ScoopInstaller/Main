{
    "homepage": "https://yarnpkg.com/",
    "license": "BSD",
    "version": "1.1.0",
    "suggest": {
        "Node.js": [
            "nodejs",
            "nodejs-lts"
        ],
        "NVM": [
            "nvm"
        ]
    },
    "url": "https://yarnpkg.com/downloads/1.1.0/yarn-1.1.0.msi",
    "hash": "601e4c4dfb66e7386aafd09b097daa3f213060762a60d00edb38d4642277da14",
    "persist": [
        "cache",
        "bin"
    ],
    "post_install": "
        yarn config delete cache # 0.24.6 | remove wrong configuration value | delete after two updates
        yarn config set cache-folder \"$persist_dir\\cache\"
        yarn config set prefix \"$persist_dir\\bin\"
    ",
    "env_add_path": [
        "bin",
        "Yarn\\bin"
    ],
    "checkver": {
        "url": "https://yarnpkg.com/latest-version",
        "re": "([\\d.]+)"
    },
    "autoupdate": {
        "url": "https://yarnpkg.com/downloads/$version/yarn-$version.msi"
    }
}
